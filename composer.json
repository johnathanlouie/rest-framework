{
    "name": "lwd/rest-framework",
    "description": "Simple PHP framework for web application and API development",
    "keywords": ["rest", "api"],
    "type": "library",
    "authors": [
        {
            "name": "Johnathan Louie",
            "email": "johnathanlouie88@gmail.com",
            "homepage": "https://jlouie.io",
            "role": "Developer"
        }
    ],
    "readme": "README.md",
    "license": "AGPL-3.0-or-later",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/johnathanlouie/http-message"
        },
        {
            "type": "vcs",
            "url": "https://github.com/johnathanlouie/http-factory"
        },
        {
            "type": "vcs",
            "url": "https://github.com/johnathanlouie/http-server-handler"
        }
    ],
    "require": {
        "php": "^5.5.1",
        "ext-json": "*",
        "lwd/http-message": "^2.0",
        "lwd/http-factory": "^1.1.0",
        "lwd/http-server-handler": "^1.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^3.7.22",
        "fzaninotto/faker": "^v1.2.0",
        "guzzlehttp/guzzle": "^v3.7.1"
    },
    "autoload": {
        "psr-4": {
            "Lwd\\RestFramework\\": "src/Lwd/RestFramework/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EndToEnd\\Lwd\\": "tests/EndToEnd/Lwd/"
        }
    }
}
